    char_col1 = char_col1, char_start1 = char_start1, char_end1 = char_end1,
    char_col2 = char_col2, char_start2 = char_start2, char_end2 = char_end2,
    char_col3 = char_col3, char_start3 = char_start3, char_end3 = char_end3,

    -- グラデーション1
